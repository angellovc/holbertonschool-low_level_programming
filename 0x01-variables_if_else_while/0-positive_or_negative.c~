#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/* more headers goes there */
/* betty style doc for function main goes there */
/*main evaluate the value of the numbers*/
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	if (n > 0)
		printf("%i is positive\n" n);
	else if (n == 0)
		printf("%i is zero\n");
	else if (n < 0)
		printf("%i is negative\n");
	return (0);
}
